<div class="container">

  <h1>Lists</h1>

  <p><%= link_to "Add a new list", new_list_path %></p>

    <% @lists.each do |list| %>
        <%= link_to list_path(list) do %>
        <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path list.photo.key, height: 300, width: 2000, crop: :fill, gravity: :face if list.photo.key %>')"><%= list.name %></div>
        <% end %>
    <% end %>
</div>
